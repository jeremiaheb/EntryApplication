
<div class='formContainer'>
  <div id="sampleSection" class="sectionDiv">
  <div class='show_section_1'>
    <div id='section_1_part_1'>

      <dl class="dl-horizontal">
        <dt>Boatlog Manager</dt><dd><%= @boat_log.boatlog_manager.agency_name %></dd>
      </dl>
      <dl class="dl-horizontal">
        <dt>Sample Date</dt><dd><%= @boat_log.date %></dd>
      </dl>
      <dl class="dl-horizontal">
        <dt>PSU</dt><dd><%= @boat_log.primary_sample_unit %></dd>
      </dl>
    </div>

    <div id='section_1_part_2'>
      <% @boat_log.boatlog_divers.each do |b_divers|  %>
       <dl class="dl-horizontal">
        <dt> <%= b_divers[2] %> </dt><dd><%= b_divers[1] %></dd>
      </dl>   
      <% end %>
    </div>
  </div>
</div>
<div class="showActions" >
 <%= button_to('Edit Boatlog', edit_boat_log_path(@boat_log), :method => "get", :class => 'btn btn-large btn-primary showAction') %>
 <%= button_to("Delete Boatlog", @boat_log, data: { confirm: 'Are you sure?' }, method: :delete, :class => 'btn btn-large btn-primary showAction') %>
 <%= button_to('Boatlog List', boat_logs_path, :method => "get", :class => 'btn btn-large btn-primary showAction') %>
</div>
